; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Luscinia"
run/main_scene="res://Scenes/resource_page.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://Sprites/icon.svg"

[display]

window/size/viewport_width=360
window/size/viewport_height=800
window/size/window_width_override=288
window/size/window_height_override=512
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/handheld/orientation=1

[input_devices]

pointing/emulate_touch_from_mouse=true

[autoload]

ResourceManager="*res://Scripts/resource_manager.gd"

[display]

window/size/viewport_width=360
window/size/viewport_height=800

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
